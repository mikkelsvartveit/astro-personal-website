---
import { Image } from "astro:assets";

interface Props {
  photo: any;
  width?: number;
  height?: number;
  class?: string;
}

const { photo, width, height, class: className } = Astro.props;
---

<Image
  src={photo}
  alt=""
  {width}
  {height}
  densities={[1, 2]}
  quality="mid"
  class={"object-contain rounded " + (className ?? "")}
  decoding="sync"
  loading="eager"
  transition:name={photo.src}
/>
